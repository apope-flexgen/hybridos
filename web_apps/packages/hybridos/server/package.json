{
  "name": "@hybridos/server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build:dev": "rimraf dist && NODE_ENV=devTools-fims-useDev nest build --webpack --webpackPath webpack.config.js",
    "build:prod": "rimraf dist && NODE_ENV=prod nest build --webpack --webpackPath webpack.config.js; node_modules/pkg/lib-es5/bin.js --targets node16-linux-x64 dist/main.js -o ./executable/web_server --config pkg.json",
    "build:debug": "rimraf dist && NODE_ENV=debug nest build --webpack --webpackPath webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "rimraf dist && NODE_ENV=dev nest build --webpack --webpackPath webpack.config.js --watch",
    "start:local": "rimraf dist && NODE_ENV=local nest build --webpack --webpackPath webpack.config.js",
    "start:debug": "nest start --debug --watch",
    "start:prod": "./build/release/nest_web_server",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --maxWorkers=1",
    "test:all": "pnpm run test && pnpm run test:e2e && pnpm run test:integration",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --maxWorkers=1 --testTimeout=10000 --config ./test/jest-e2e.json",
    "test:integration": "jest --maxWorkers=50% --testTimeout=30000 --config ./test/jest-integration.json --silent",
    "migrate:up": "bash ./migrate-mongo/migrate-up.sh",
    "migrate:down": "bash ./migrate-mongo/migrate-down.sh"
  },
  "dependencies": {
    "@flexgen/fims": "^1.4.0",
    "@flexgen/storybook": "^0.2.13",
