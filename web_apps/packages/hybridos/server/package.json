    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@testing-library/dom": ">=7.21.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.9",
    "@types/jest": "^28.1.4",
