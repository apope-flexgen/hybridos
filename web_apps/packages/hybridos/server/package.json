    "migrate-mongo": "^9.0.0",
    "mongodb": "^4.10.0",
    "mongodb-memory-server": "^8.9.3",
    "mongodb-memory-server-core": "^8.9.3",
    "mongodb-memory-server-global-4.4": "^8.9.3",
    "mongoose": "^6.8.0",
