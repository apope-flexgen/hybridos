{
    "input_path"    : "/home/data",
    "failed_validate_path" : "/home/validate_error",
    "failed_write_path" : "/home/write_error",
    "forward" : "",
    "ext"     : [".tar.gz", ".parquet.gz"],
    "num_validate_workers": 4,
    "num_influx_prepare_batches_workers": 4,
    "num_influx_send_batches_workers": 4,
    "db_health_check_delay_seconds": 0.05,
    "influx_address" : "1.1.2.2:8086",
    "mongo_address" : "1.1.2.1:27017",
    "retention_policies": [
        {
            "duration": "INF",
            "name": "infinite_rp"
        },
        {
            "duration": "30d",
            "default": true,
            "name": "30_day_rp"
        },
        {
            "duration": "120d",
            "name": "120_day_rp"
        }
    ],
    "continuous_queries": [
        {
            "group_by": "time(10m), *",
            "resample": "EVERY 10m FOR 20m",
            "name": "10min_cq",
            "rp": "infinite_rp",
            "select": "MIN(*), MAX(*), MEAN(*), LAST(*)"
        },
        {
            "group_by": "time(1m), *",
            "resample": "EVERY 5m FOR 10m",
            "name": "1min_cq",
            "rp": "120_day_rp",
            "select": "MIN(*), MAX(*), MEAN(*), LAST(*)"
        }
    ]
}