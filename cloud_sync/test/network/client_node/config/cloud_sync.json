{
    "clients": {
        "main" : {
            "directory" : "/home/archives",
            "private_key" : "/root/.ssh/id_rsa",
            "known_hosts" : "/root/.ssh/known_hosts",
            "extension" : ".tar.gz"
        }
    },
    "servers": {
        "destination": {
            "ip" : "1.1.1.2",
            "port": "22",
            "user": "root",
            "directory" : "/home/archives",
            "timeout" : 2
        },
        "local_consumer": {
            "directory" : "/home/local_consumer"
        }
    },
    "db_directory" : "/home/.cloud_sync/db",
    "retry_limit" : 1,
    "sleep_limit_seconds" : 4,
    "buffer_size" : 100000
}