{
    "env": {
        "browser": true
    },
    "parser": "babel-eslint",
    "rules": {
        "no-console": "off",
        "linebreak-style": ["error", "windows"]
    },
    "plugins": [
        "react"
    ],
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "extends": [
        "airbnb-base",
        "plugin:react/recommended",
        "prettier"
    ]
}